{
  "version": 2,
  "builds": [
    { "src": "app.py", "use": "@vercel/python" },
    { "src": "static/tiktok*.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/static/tiktokkwvcKuW5hmfajEk4rJr2vJUWNNGU4W2x.html",
      "headers": {
        "Content-Type": "text/plain",
        "Cache-Control": "no-store"
      }
    },
    { "src": "/(.*)", "dest": "app.py" }
  ]
}
